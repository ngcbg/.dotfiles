xterm*color15:					#eceff4

xterm*renderFont:				true
xterm*font:						-xos4-terminus-medium-r-*-*-16-*-*-*-*-*-iso10646-*
xterm*faceName:					-xos4-terminus-medium-r-*-*-16-*-*-*-*-*-iso10646-*
xterm*faceSize:					12
xterm*loginShell:				true
xterm*highlightSelection:		true
xterm*trimSelection:			true				
XTerm*borderWidth:				0					
XTerm*internalBorder:			2
XTerm*termName:					xterm-256color
XTerm*vt100.metaSendsEscape:	true				
XTerm*v100.saveLines:			10240
XTerm*vt100.scrollBar:			false
XTerm*vt100.bellIsUrgent:		true
XTerm*allowBoldFonts:			false
xterm*colorBDMode:				true				
xterm*colorBD:					rgb:fc/fc/fc
XTerm*scrollKey:				true				
XTerm*fullscreen:				never				
XTerm*cutToBeginningOfLine:		false				
XTerm*cutNewline:				false				
XTerm*on2Clicks:				word				! select word on two clicks
! XTerm*on2Clicks:				regex [^ \n()]+
XTerm*on3Clicks:				line				! select whole line on three clicks
XTerm.selectToClipboard:		true
*VT100.Translations:			#override \
    Shift Ctrl<Key>V:			insert-selection(PRIMARY) \n\
    Shift<Btn1Down>:			select-start() \n\
    Shift<Btn1Motion>:			select-extend() \n\
    Shift<Btn1Up>:				select-end()
!*ttyModes:						erase ^?
XTerm.vt100.geometry:			100x35
xterm*xftAntialias:				true
XTerm*charClass:				33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48 

! ------------------------------------------------------------------------------
! UXTerm
! ------------------------------------------------------------------------------
UXTerm*termName:                xterm-256color
!UXTerm*cursorColor:            white
UXTerm*VT100.geometry:          100x35
UXTerm.faceSize:                16
UXTerm*dynamicColors:           true
UXTerm*utf8:                    2
UXTerm*eightBitInput:           true
UXTerm*saveLines:               10000
UXTerm*scrollKey:               true
UXTerm*scrollTtyOutput:         false
UXTerm*scrollBar:               false
UXTerm*rightScrollBar:          false
UXTerm*jumpScroll:              true
UXTerm*multiScroll:             true
UXTerm*toolBar:                 false

!-------------------------------------------------------------------------------
! Urxvt
!-------------------------------------------------------------------------------
URxvt*loginShell:					true
URxvt*scrollTtyKeypress:			true
URxvt*scrollTtyOutput:				false
URxvt*skipScroll:					true

! URXVT APPEARANCE
!URxvt.font:							xft:JetBrainsMono Nerd Font Mono:style:Regular:size=13:antialias=true
URxvt.font:							xft:Hack Nerd Font Mono:style=Regular:size=13:antialias=true,xft:JetBrainsMono Nerd Font Mono:style:Regular:size=13:antialias=true
URxvt.boldFont:						xft:Hack NF:style=Bold:size=13:antialias=true
URxvt.italicFont:					xft:Hack NF:style=Italic:size=13:antialias=true
URxvt.boldItalicFont:				xft:Hack NF:style=Bold Italic:size=13:antialias=true
URxvt.allow_bold:					true
URxvt.letterSpace:					-1
URxvt.lineSpace:					0
!URxvt.geometry:					100x35
URxvt.internalBorder:				5
URxvt.cursorBlink:					false
URxvt.cursorUnderline:				false
URxvt.saveline:						32768
URxvt.scrollBar:					false
URxvt.scrollBar_right:				true
URxvt.scrollBar_floating:			false
URxvt.scrollstyle:					plain
URxvt.urgentOnBell:					false
URxvt.depth:						24
URxvt.iso14755:						false
URxvt.iconFile:						/usr/share/icons/High-Delight/apps/utilities-terminal.svg

! URXVT EXTENSIONS
URxvt.perl-ext-common:				default,clipboard,keyboard-select,url-select,tabbedex
URxvt.tabbedex.no-tabbedex-keys:	yes
URxvt.tabbedex.new-button:			false
URXvt.tabbedex.reopen-on-close:		yes
URxvt.tabbedex.autohide:			yes
URxvt.tabbedex.tabbar-fg:			1
URxvt.tabbedex.tabbar-bg:			0
URxvt.tabbedex.tab-fg:				4
URxvt.tabbedex.tab-bg:				0
URxvt.tabbedex.bell-fg:				0
URxvt.tabbedex.bell-bg:				0
URxvt.tabbedex.bell-tab-fg:       	0
URxvt.tabbedex.bell-tab-bg:       	0
URxvt.tabbedex.title-fg:          	7
URxvt.tabbedex.title-bg:          	0
URxvt.keysym.Control-Shift-T:     	perl:tabbedex:new_tab
URxvt.keysym.Control-Shift-R:     	perl:tabbedex:rename_tab
URxvt.keysym.Control-Shift-W:     	perl:tabbedex:kill_tab
!URxvt.keysym.Control-Shift-Next:  	perl:tabbedex:move_tab_right
!URxvt.keysym.Control-Shift-Prior: 	perl:tabbedex:move_tab_left
URxvt.keysym.Control-Shift-Right: 	perl:tabbedex:next_tab
URxvt.keysym.Control-Shift-Left:  	perl:tabbedex:prev_tab
URxvt.clipboard.copycmd:          	xclip -i -selection clipboard
URxvt.clipboard.pastecmd:         	xclip -o -selection clipboard
URxvt.keysym.M-c:                 	perl:clipboard:copy
URxvt.keysym.M-v:                 	perl:clipboard:paste
URxvt.keysym.M-Escape:            	perl:keyboard-select:activate
URxvt.keysym.M-s:                 	perl:keyboard-select:search
URxvt.urlLauncher:                	luakit
URxvt.underlineURLs:              	true
URxvt.urlButton:                  	1

