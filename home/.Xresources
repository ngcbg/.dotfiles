xterm*foreground:				#d8dee9
xterm*background :				#2e3440
xterm*cursorColor:				#d8dee9
!xterm*cursorColor:				#8AE234
xterm*colorUL:					#366060
xterm*underlineColor:			#92659a
xterm*fading:					35
xterm*fadeColor:				#4c566a
xterm*color0:					#3b4252
xterm*color1:					#bf616a
xterm*color2:					#a3be8c
xterm*color3:					#ebcb8b
