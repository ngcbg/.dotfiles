#--------------------------------------------------------

[module/date]
type = internal/date
interval = 5
date =
date-alt = " %m.%d.%Y"
time = %H:%M
time-alt = %A %H:%M
format-prefix = " "
format-prefix-foreground = ${colors.foreground}
label = %date% %time%
format-underline = #95ffb52a

#--------------------------------------------------------

[module/pulseaudio]
type = internal/pulseaudio
