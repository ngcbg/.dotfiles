################################################################################
############                    MAINBAR-OPENBOX                     ############
################################################################################

[bar/mainbar-openbox]
monitor-fallback = HDMI-0
monitor = ${env:MONITOR:DP-4}
wm-name = openbox
width = 100%
height = 28
bottom = false
#offset-x = 1%
#offset-y = 1%
radius = 0.0
fixed-center = true

background = ${colors.background}
foreground = ${colors.foreground}
line-size = 2
line-color = #f00

#override-redirect = true
#wm-restack = i3
screenchange-reload = true 

padding-left = 1
padding-right = 1
module-margin-left = 2
module-margin-right = 2

# Enable support for inter-process messaging
enable-ipc = true

font-0 = "fixed:pixelsize=12;2"
font-1 = "Feather:size=12;2"
font-2 = "icomoon:pixelsize=13;3"
font-3 = "Symbols Nerd Font:style:1000-em:pixelsize=13;3"
font-4 = "DejaVu Sans:size:13;1"

modules-left = menu xworkspaces cpu load-average cpu-temp cpu-fan gpu-temp memreal
modules-center = xwindow filesystem
modules-right = window_switch wired-network xkeyboard date

tray-position = right
tray-detached = false
tray-offset-x = 0
tray-offset-y = 0
tray-padding = 2
tray-maxsize = 20
tray-scale = 1.0
tray-background = ${colors.background}

cursor-click = pointer
cursor-scroll = ns-resize

